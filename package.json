{
  "name": "@rubixibuc/with-styles",
  "version": "0.1.12",
  "description": "react style hoc",
  "main": "dist/with-styles.js",
  "scripts": {
    "build": "webpack",
    "prettier": "prettier --single-quote --write \"src/**/*.{js,jsx,json,css}\" ./webpack.config.js",
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test-dev": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubixibuc/with-styles.git"
  },
  "keywords": [
    "react",
    "style",
    "hoc"
  ],
  "author": "rubixibuc",
  "license": "ISC",
  "dependencies": {
    "lodash": ">=0.1.0"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.1.0",
    "prettier": "^1.12.0",
    "react": "^16.3.2",
    "react-dom": "^16.4.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-node-externals": "^1.7.2"
  },
  "files": [
    "dist/with-styles.js"
  ],
  "jest": {
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
